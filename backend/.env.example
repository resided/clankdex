# Server Configuration
PORT=3001

# Network (mainnet or sepolia)
NETWORK=sepolia

# Blockchain RPC URLs
RPC_URL=https://sepolia.base.org
BASE_RPC_URL=https://mainnet.base.org
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# Deployment
DEPLOYER_PRIVATE_KEY=your_private_key_here

# Clanker Configuration
CLANKER_VAULT=0xa1da0600Eb4a9F3D4a892feAa2c2caf80A4A2f14
CLADEX_ADMIN=0x...
CLADEX_RECIPIENT=0x...

# Simulation mode (set to 'true' for testing without real deployment)
SIMULATE_DEPLOY=false

# API Keys
BASESCAN_API_KEY=your_basescan_api_key
PINATA_JWT=your_pinata_jwt
PINATA_GATEWAY=gateway.pinata.cloud

# Optional: OpenAI for enhanced generation
OPENAI_API_KEY=your_openai_api_key
